
{
    "Warehouses":{
        "_id": {
            "regexp" : "[1-9][A-Z][0-9][A-Za-z]{2}[0-9][a-z]{10,20}"
        },
        "name":{
            "string" : ["Warehouse 1","Warehouse 2","Warehouse 3","Warehouse 4"]
        }
    },
    
    "Products":{
        "_id": {
            "regexp" : "[1-9]{13}[A-Za-z]{2}"
        },
        "barCode": {
            "regexp" : "[1-9]{13}"
        },
        "name":{
            "regexp" : "[Product][0-9]{3}"
        },
        "warehouses" : {
            "hasMany" : "Warehouses"
        },
        "totalStock" :{
            "int" : { "min": 0, "max": 9999 }
        }
       
    },
    "Users":{
        "_id": {
            "regexp" : "[1-9][A-Z][0-9][A-Za-z]{2}[0-9][a-z]{10,20}"
        },
        "name":{
            "regexp" : "[User][0-9]{3}"
        },
        "email": { 
            "regexp": "[a-z]{5,10}@[a-z]{5}\\.[a-z]{2,3}" 
        },
        "isAdmin": { 
            "boolean": {} 
        } 
    }
    
}
    

